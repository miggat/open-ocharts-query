{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug open-ocharts-query with xvfb",
      "type": "cppdbg",
      "request": "launch",
      "program": "/home/pi/projects/open-ocharts-query/build/open-ocharts-query",
      "args": [
        "/home/pi/Desktop/charts_decrypted",
        "10689"
      ],
      
      "stopAtEntry": false,
      "cwd": "/home/pi/projects/open-ocharts-query",
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}